    "declaration": true,
    "declarationDir": "./dist",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": false,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
